{
  "compilerOptions": {
    
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "prisma"]
}
